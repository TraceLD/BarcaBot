{
  "name": "BarcaBot.Website",
  "description": "BarcaBot's Website",
  "version": "1.0.0",
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "cross-env NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build": "npm run build:tailwind && sapper build --legacy",
    "dev": "sapper dev",
    "export": "npm run build:tailwind && sapper export --legacy",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "cross-env": "^7.0.2",
    "postcss-cli": "^8.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "tailwindcss": "^1.8.10"
  }
}
